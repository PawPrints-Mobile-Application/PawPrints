{
  "name": "pawprints",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview --port 1000",
    "preview:host": "vite preview --host --port 1000",

    "host": "vite dev --host --port 2222",
    "ionic:build": "npm run build",
    "ionic:serve": "vite dev --host --port 3000",

    "android" : "ionic cap run android --livereload --host --port 4000",
    "android:host" : "ionic cap run android --livereload --external --port 4001",
    "android:host:phone": "ionic cap run android --target adb-311ec259-wDOIV1._adb-tls-connect._tcp",
    
    "web" : "npm run dev --livereload --host --port 5000",
    "web:host" : "vite dev --host --port 5001"
  },
  "dependencies": {
    "@capacitor/android": "^5.4.1",
    "@capacitor/app": "5.0.6",
    "@capacitor/cli": "5.4.1",
    "@capacitor/core": "5.4.1",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/ios": "5.4.1",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@ionic/cli": "^7.1.1",
    "@ionic/vue": "^7.4.1",
    "@ionic/vue-router": "^7.4.1",
    "@vue/cli": "^5.0.8",
    "firebase": "^10.4.0",
    "firebase-tools": "^12.6.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  }
}
